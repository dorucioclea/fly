{
  "name": "@fly/cli",
  "version": "0.54.1-api-v2.0",
  "description": "fly cli",
  "homepage": "https://github.com/superfly/fly#readme",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "bin": {
    "fly": "./bin/run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/superfly/fly"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "yarn oclif-dev manifest && yarn oclif-dev readme",
    "version": "yarn oclif-dev readme && git add README.md"
  },
  "bugs": {
    "url": "https://github.com/superfly/fly/issues"
  },
  "dependencies": {
    "@fly/build": "0.54.0",
    "@fly/core": "0.54.0",
    "@fly/examples": "0.50.0",
    "@fly/v8env": "0.54.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-update": "^1.3.9",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "axios": "^0.18.0",
    "chalk": "^2.4.2",
    "cli-ux": "^5.2.1",
    "execa": "^0.7.0",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "inquirer": "^6.2.2",
    "js-yaml": "^3.13.0",
    "netrc-parser": "^3.1.6",
    "node-fetch": "^2.6.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/execa": "^0.7.0",
    "@types/inquirer": "^0.0.44",
    "@types/node": "^10",
    "aws-sdk": "^2.428.0",
    "globby": "^8",
    "ts-node": "^8",
    "tslint": "^5"
  },
  "oclif": {
    "description": "Fly edge application runtime",
    "commands": "./lib/commands",
    "bin": "fly",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "macos": {
      "identifier": "com.superfly.fly"
    },
    "update": {
      "s3": {
        "host": "https://get.fly.io",
        "bucket": "flyio-cli-assets",
        "xz": false,
        "templates": {
          "target": {
            "unversioned": "tarballs/<%- channel %>/<%- bin %>-<%- platform %>-<%- arch %><%- ext %>",
            "versioned": "tarballs/<%- channel %>/<%- bin %>-v<%- version %>/<%- bin %>-v<%- version %>-<%- platform %>-<%- arch %><%- ext %>",
            "manifest": "tarballs/<%- channel %>/<%- platform %>-<%- arch %>"
          },
          "vanilla": {
            "unversioned": "tarballs/x/<%- channel %>/<%- bin %><%- ext %>",
            "versioned": "tarballs/x/<%- channel %>/<%- bin %>-v<%- version %>/<%- bin %>-v<%- version %><%- ext %>",
            "manifest": "tarballs/x/<%- channel %>/version"
          }
        }
      },
      "node": {
        "version": "11.10.1",
        "targets": [
          "darwin-x64",
          "linux-x64"
        ]
      }
    },
    "warn-if-update-available": {
      "timeoutInDays": 1,
      "message": "fly update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>."
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=11.10.0"
  }
}
